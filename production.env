# Production Environment Variables for Moose

# Node.js Configuration
NODE_ENV=production
NODE_OPTIONS="--max-old-space-size=2048"

# TypeScript/ts-patch Configuration
TS_NODE_TRANSPILE_ONLY=true
TS_NODE_FILES=true
TS_PATCH_DISABLE_CACHE=true

# Moose Configuration
MOOSE_ENV=production
MOOSE_LOG_LEVEL=info

# Performance Tuning
UV_THREADPOOL_SIZE=16
NODE_MAX_WORKERS=4

# Memory Management
NODE_GC_INTERVAL=100
